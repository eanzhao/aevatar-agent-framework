{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Orleans": "Warning"
    }
  },
  "StreamingOptions": {
    "DefaultStreamNamespace": "KafkaConfigTest",
    "StreamProviderName": "StreamProvider",
    "AllowCustomNamespaces": false
  },
  "Kafka": {
    "Brokers": [
      "localhost:9092"
    ],
    "ConsumerGroupId": "config-test-consumer-group",
    "ConsumeMode": "LastCommittedMessage",
    "PollTimeoutMs": 100,
    "Topics": [
      {
        "Name": "KafkaConfigTest",
        "Partitions": 16,
        "ReplicationFactor": 1,
        "AutoCreate": true
      }
    ],
    "Performance": {
      "GetQueueMsgsTimerPeriodMs": 50,
      "MessageBatchSize": 100
    }
  }
}

