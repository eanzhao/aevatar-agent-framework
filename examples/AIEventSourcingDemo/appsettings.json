{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "ConnectionStrings": {
    "MongoDB": "mongodb://localhost:27017"
  },
  "LLMProviders": {
    "Providers": {
      "azure-gpt35": {
        "ProviderType": "AzureOpenAI",
