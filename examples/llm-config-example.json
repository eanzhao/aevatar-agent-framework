{
  "LLMProviders": {
    "default": "openai-gpt4",
    "providers": {
      "openai-gpt4": {
        "providerType": "OpenAI",
        "apiKey": "${OPENAI_API_KEY}",
        "model": "gpt-4",
        "temperature": 0.7,
        "maxTokens": 2000,
        "enableStreaming": false
      },
      "openai-gpt35": {
        "providerType": "OpenAI",
        "apiKey": "${OPENAI_API_KEY}",
        "model": "gpt-3.5-turbo",
        "temperature": 0.5,
        "maxTokens": 1500,
        "enableStreaming": false
      },
      "azure-gpt35": {
        "providerType": "AzureOpenAI",
        "apiKey": "${AZURE_OPENAI_API_KEY}",
        "endpoint": "https://your-resource.openai.azure.com",
        "deploymentName": "gpt-35-turbo",
        "model": "gpt-3.5-turbo",
        "temperature": 0.7,
        "maxTokens": 2000
      },
      "azure-gpt4": {
        "providerType": "AzureOpenAI",
        "apiKey": "${AZURE_OPENAI_API_KEY}",
        "endpoint": "https://your-resource.openai.azure.com",
        "deploymentName": "gpt-4",
        "model": "gpt-4",
        "temperature": 0.7,
        "maxTokens": 4000
      },
      "local-llama": {
        "providerType": "Ollama",
        "endpoint": "http://localhost:11434",
        "model": "llama2:70b",
        "temperature": 0.8,
        "maxTokens": 2000
      },
      "local-codellama": {
        "providerType": "Ollama",
        "endpoint": "http://localhost:11434",
        "model": "codellama:34b",
        "temperature": 0.2,
        "maxTokens": 3000
      },
      "anthropic-claude": {
        "providerType": "Anthropic",
        "apiKey": "${ANTHROPIC_API_KEY}",
        "model": "claude-3-opus-20240229",
        "temperature": 0.7,
        "maxTokens": 4000
      }
    }
  }
}
