{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Aevatar": "Debug"
    }
  },
  "LLMProviders": {
    "Default": "test-provider",
    "Providers": {
      "test-provider": {
        "Name": "test-provider",
        "ProviderType": "test",
        "Model": "test-model",
        "ApiKey": "test-key-from-config",
        "Temperature": 0.5,
        "MaxTokens": 1024,
        "TimeoutSeconds": 30
      },
      "openai-provider": {
        "Name": "openai-provider",
        "ProviderType": "openai",
        "Model": "gpt-4-turbo",
        "ApiKey": "fake-openai-key",
        "Temperature": 0.7,
        "MaxTokens": 4096,
        "TimeoutSeconds": 60
      },
      "azure-provider": {
        "Name": "azure-provider",
        "ProviderType": "azure",
        "Model": "gpt-35-turbo",
        "DeploymentName": "gpt35-turbo-deployment",
        "ApiKey": "fake-azure-key",
        "Endpoint": "https://test.openai.azure.com/",
        "Temperature": 0.3,
        "MaxTokens": 2048,
        "TimeoutSeconds": 45
      }
    }
  },
  "AIAgents": {
    "MaxChainOfAevatarThoughtSteps": 10,
    "MaxReActIterations": 15,
    "DefaultProcessingMode": "Auto",
    "EnableTelemetry": false,
    "EnableCaching": true
  },
  "TestSettings": {
    "UseRealProviders": false,
    "MockDelay": 100,
    "EnableDetailedLogging": true
  }
}

